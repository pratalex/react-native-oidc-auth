{
  "name": "react-native-oidc-auth-core",
  "version": "0.1.1",
  "main": "dist/index.js",
  "description": "Framework-agnostic core with login, logout, refresh, and secure token storage contracts.",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "targets": [
    [
      "module",
      {
        "esm": true
      }
    ],
    "typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && resolve-tspaths",
    "prepack": "npm run build",
    "test": "jest",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "axios": "*",
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "axios-auth-refresh": "^3.3.6",
    "jwt-decode": "^4.0.0",
    "tiny-emitter": "2.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.27.1",
    "@react-native/babel-preset": "0.80.2",
    "@testing-library/react-native": "13.3.3",
    "@types/jest": "30.0.0",
    "@types/luxon": "3.7.1",
    "@types/react": "^19.2.2",
    "@types/react-native": "^0.73.0",
    "axios": "^1.12.2",
    "eslint": "^9.37.0",
    "eslint-config-expo": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "expo-jwt": "1.8.2",
    "jest": "29.7.0",
    "luxon": "3.7.2",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-test-renderer": "19.1.0",
    "resolve-tspaths": "0.8.23",
    "typescript": "5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "Alexandre PRAT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pratalex/react-native-oidc-auth.git",
    "directory": "packages/react-native-oidc-auth-core"
  },
  "bugs": {
    "url": "https://github.com/pratalex/react-native-oidc-auth/issues"
  },
  "gitHead": "216d5f6e61eeb6570b2dd1d766a69f3ca98a4243"
}
